<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#a2c5e8"
    tools:context=".page_info_personnel">


    <Button
        android:id="@+id/continueD"
        android:layout_width="287dp"
        android:layout_height="46dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="28dp"
        android:backgroundTint="#305E96"
        android:paddingHorizontal="60dp"
        android:text="View Information"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4"
        app:layout_constraintVertical_bias="0.0" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="282dp"
        android:layout_height="453dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="4dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/user"
        app:layout_constraintTop_toBottomOf="@+id/imageView8">

        <EditText
            android:id="@+id/e1"
            android:layout_width="261dp"
            android:layout_height="43dp"
            android:layout_margin="15dp"
            android:layout_marginStart="21dp"
            android:layout_marginTop="59dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="83dp"
            android:backgroundTint="#000000"
            android:drawablePadding="8dp"
            android:inputType="text|textPostalAddress|textMultiLine"
            android:paddingHorizontal="30dp" />

        <EditText
            android:id="@+id/e2"
            android:layout_width="261dp"
            android:layout_height="43dp"
            android:layout_margin="15dp"
            android:layout_marginStart="47dp"
            android:layout_marginEnd="12dp"
            android:backgroundTint="#000000"
            android:drawablePadding="8dp"
            android:inputType="text|textPostalAddress|textMultiLine"
            android:paddingHorizontal="30dp" />

        <EditText
            android:id="@+id/e3"
            android:layout_width="261dp"
            android:layout_height="43dp"
            android:layout_margin="15dp"
            android:layout_marginStart="22dp"
            android:layout_marginEnd="12dp"
            android:backgroundTint="#000000"
            android:drawablePadding="8dp"
            android:inputType="text|textPostalAddress|textMultiLine"
            android:paddingHorizontal="30dp" />

        <EditText
            android:id="@+id/e4"
            android:layout_width="261dp"
            android:layout_height="43dp"
            android:layout_margin="15dp"
            android:layout_marginStart="33dp"
            android:layout_marginEnd="12dp"
            android:backgroundTint="#000000"
            android:drawablePadding="8dp"
            android:inputType="text|textPostalAddress|textMultiLine"
            android:paddingHorizontal="30dp" />

        <EditText
            android:id="@+id/e5"
            android:layout_width="261dp"
            android:layout_height="43dp"
            android:layout_margin="15dp"
            android:layout_marginStart="26dp"
            android:layout_marginEnd="24dp"
            android:backgroundTint="#000000"
            android:drawablePadding="8dp"
            android:inputType="text|textPostalAddress|textMultiLine"
            android:paddingHorizontal="30dp" />

        <EditText
            android:id="@+id/e6"
            android:layout_width="261dp"
            android:layout_height="43dp"
            android:layout_margin="15dp"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="24dp"
            android:backgroundTint="#000000"
            android:drawablePadding="8dp"
            android:inputType="text|textPostalAddress|textMultiLine"
            android:paddingHorizontal="30dp" />

    </LinearLayout>

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="181dp"
        android:layout_height="205dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/shooow" />

    <TextView
        android:id="@+id/inf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginTop="61dp"
        android:layout_marginEnd="11dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="Information "
        android:textColor="#305E96"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/per"
        app:layout_constraintEnd_toStartOf="@+id/imageView8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/per"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="51dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="163dp"
        android:fontFamily="@font/montserrat_bold"
        android:text="Personnel"
        android:textColor="#305E96"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout4"
        app:layout_constraintEnd_toStartOf="@+id/imageView8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inf" />

    <TextView
        android:id="@+id/full"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="302dp"
        android:layout_marginBottom="25dp"
        android:text=" FullName"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/per"
        app:layout_constraintVertical_bias="0.857" />

    <TextView
        android:id="@+id/email"
        android:layout_width="85dp"
        android:layout_height="29dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="302dp"
        android:layout_marginBottom="40dp"
        android:text="Email"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/user"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="296dp"
        android:layout_marginBottom="40dp"
        android:text=" Username"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/adress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.992" />

    <TextView
        android:id="@+id/adress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="314dp"
        android:layout_marginBottom="50dp"
        android:text=" Adresse"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="254dp"
        android:text="Phone NB"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.677" />

    <TextView
        android:id="@+id/gender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="323dp"
        android:text="Gender"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone" />


</androidx.constraintlayout.widget.ConstraintLayout>